<html>
    <head></head>
    <body>
        <div>
            <h1>Inventory</h1>
            
            <table style="border:1px solid red;padding: 10px;text-align: center;">
                <tr>
                    <td style="border:1px solid red;padding: 10px;">Inventory_Id</td>
                    <td style="border:1px solid red;padding: 10px;">Inventory_Type</td>
                    <td style="border:1px solid red;padding: 10px;">Item_Name</td>
                    <td style="border:1px solid red;padding: 10px;">Available_Quantity</td>
                </tr>
                <% for(let ele of inventory) { %>
                <tr >
                    <td><%= ele.inventory_id%></td>
                    <td><%= ele.inventory_type %></td>
                    <td><%= ele.item_name %></td>
                    <td><%= ele.available_quantity%></td>
                </tr>
                <% } %>
            </table>
           
        </div>
    </body>
</html>